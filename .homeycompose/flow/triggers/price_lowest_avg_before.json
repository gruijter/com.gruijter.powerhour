			},
			"placeholder": { "en": "2-8" },
			"min": 2,
			"max": 8,
			"step": 1
		},
		{
			"type": "number",
			"name": "period",
			"title": { 
				"en": "period",
				"de": "Zeitraum",
				"sv": "period",
				"it": "periodo",
				"no": "periode",
				"nl": "periode"
			},
			"placeholder": { "en": "2 - 23" },
			"min": 2,
			"max": 23,
			"step": 1
		},
		{
			"type": "number",
			"name": "time",
			"title": { 
				"en": "time",
				"de": "Zeit",
				"sv": "tid",
				"it": "tempo",
				"no": "tid",
				"nl": "tijd"
			},
			"placeholder": { "en": "01 - 24" },
			"min": 1,
			"max": 24,
			"step": 1
