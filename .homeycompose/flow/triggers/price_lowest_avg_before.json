	],
	"tokens": [
		{
		"name": "meter_price_h0",
		"title": {
			"en": "Price now",
			"de": "Preis aktuell",
			"sv": "Pris just nu",
			"it": "Prezzo attuale",
			"no": "Pris nå",
			"nl": "Prijs nu"
		},
		"type": "number",
		"example": 27.5
