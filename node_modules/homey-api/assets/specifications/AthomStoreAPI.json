          "type": "string",
          "in": "body"
        },
        "packageName": {
          "type": "string",
          "in": "body"
        },
        "athomId": {
          "type": "string",
          "in": "body"
        },
        "quantity": {
          "type": "number",
          "in": "body"
        },
        "duration": {
          "type": "string",
          "in": "body"
        }
      }
    },
    "restoreSubscriptions": {
      "path": "/subscription/restore",
      "method": "get",
      "parameters": {}
