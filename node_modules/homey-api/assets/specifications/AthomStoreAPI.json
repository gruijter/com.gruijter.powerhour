    "verifySubscription": {
      "path": "/subscription/verify",
      "method": "post",
      "parameters": {
        "token": {
          "type": "string",
          "in": "body"
        },
        "sku": {
          "type": "string",
          "in": "body"
        },
        "packageName": {
          "type": "string",
          "in": "body"
        },
        "receipt": {
          "type": "string",
          "in": "body"
        }
      }
    },
    "deferSubscription": {
      "path": "/subscription/defer",
      "method": "post",
