        "expiresAt": {
          "type": "string",
          "in": "body"
        },
        "campaignId": {
          "type": "string",
          "in": "body"
        },
        "subscriptionType": {
          "type": "string",
          "in": "body"
        },
        "subscriptionQuantity": {
          "type": "number",
          "in": "body"
        },
        "subscriptionDuration": {
          "type": "string",
          "in": "body"
        },
        "maxRedeems": {
          "type": "number",
          "in": "body"
        }
